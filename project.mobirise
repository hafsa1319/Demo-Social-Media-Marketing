ay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.4\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-{{textWidth}}\" mbr-class=\"{'col-lg-12':fullWidth}\">\n                <div class=\"mbr-overlay\" mbr-if=\"overlaySlide\" mbr-style=\"{'opacity': overlaySlideOpacity, 'background-color': overlaySlideColor}\">\n                </div>\n                <div class=\"row g\">\n                    <div class=\"card col-12 col-md-4\">\n                        <div class=\"card-wrapper\">\n                            <div class=\"card-img card1\">\n                            </div>\n                            <div class=\"card-box\">\n                                <h4 class=\"card-title align-center mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">Web\n                                    Design</h4>\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"card col-12 col-md-4\">\n                        <div class=\"card-wrapper\">\n                            <div class=\"card-img card2\">\n                            </div>\n                            <div class=\"card-box\">\n                                <h4 class=\"card-title align-center mbr-white mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                                    Marketing</h4>\n                            </div>\n                        </div>\n    